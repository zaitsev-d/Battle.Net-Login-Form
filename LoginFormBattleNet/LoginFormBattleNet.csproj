    </Resource>
    <Resource Include="Assets\cross-deactive.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\globus-active.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
