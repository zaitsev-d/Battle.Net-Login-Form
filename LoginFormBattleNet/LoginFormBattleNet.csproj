    <None Remove="Assets\cross-active.png" />
    <None Remove="Assets\cross-deactive.png" />
    <None Remove="Assets\globus-active.png" />
    <None Remove="Assets\globus-deactive.png" />
    <None Remove="Assets\icon.ico" />
    <None Remove="Assets\icon.png" />
    <None Remove="Assets\logo.png" />
    <None Remove="Assets\wm-active.png" />
    <None Remove="Assets\wm-deactive.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\angle-down-active.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\angle-down-deactive.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\cog-active.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
